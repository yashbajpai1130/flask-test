{
    "version" : 2, 
    "builds" : [
        {
            "src" : "index.py", 
            "use" : "@vercel/python",
            "config" : {"runtime" : "python3.8"}
        }
    ], 
    "routes" : [
        {
            "src" : "/(.*)", 
            "dest" : "index.py"
        }, 
        {
            "src" : "/submit", 
            "method" : ["POST"], 
            "dest" : "index.py"
        }
    ]
}